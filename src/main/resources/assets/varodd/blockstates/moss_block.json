{
    "multipart":
    [
	    {
	    	"apply": 
	    	[
	    		{ "model": "varodd:block/moss_block" },
	    		{ "model": "varodd:block/moss_block", "y": 90 },
	    		{ "model": "varodd:block/moss_block", "y": 180 },
	    		{ "model": "varodd:block/moss_block", "y": 270 }
	    	]
    	},
    	{
        	"when": { "north": "true" },
    		"apply":{ "model": "varodd:block/moss_block_tendrils", "x": 90 }
    	},
    	{
        	"when": { "south": "true" },
    		"apply":{ "model": "varodd:block/moss_block_tendrils", "x": 270 }
    	},
    	{
        	"when": { "east": "true" },
    		"apply":{ "model": "varodd:block/moss_block_tendrils", "x": 90, "y": 90 }
    	},
    	{
        	"when": { "west": "true" },
    		"apply":{ "model": "varodd:block/moss_block_tendrils", "x": 90, "y": 270 }
    	},
    	{
        	"when": { "up": "true" },
    		"apply":{ "model": "varodd:block/moss_block_tendrils" }
    	},
    	{
        	"when": { "down": "true" },
    		"apply":{ "model": "varodd:block/moss_block_tendrils", "x": 180 }
    	}
    ]
}
